    "# Densidad de la distribuci√≥n normal te√≥rica\n",
    "x = np.linspace(-3, 3, 1000)  # Valores para la curva\n",
    "y = norm.pdf(x, 0, 1)  # Densidad de la distribuci√≥n normal\n",
    "\n",
    "# Superponer la curva de la distribuci√≥n normal te√≥rica\n",
    "plt.plot(x, y, color='black')\n",
